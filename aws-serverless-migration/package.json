{
  "name": "ecommerce-serverless",
  "version": "1.0.0",
  "description": "Serverless ecommerce backend",
  "main": "handler.js",
  "scripts": {
    "dev": "serverless offline",
    "deploy": "serverless deploy",
    "deploy:dev": "serverless deploy --stage dev",
    "deploy:prod": "serverless deploy --stage prod",
    "remove": "serverless remove",
    "logs": "serverless logs -f",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "build": "tsc",
    "db:setup": "node scripts/setup-database.js",
    "db:init": "npm run db:setup",
    "clean": "rm -rf .webpack .serverless dist node_modules/.cache"
  },
  "dependencies": {
    "@google/generative-ai": "^0.17.1",
    "aws-sdk": "^2.1691.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "lambda-multipart-parser": "^1.0.1",
    "mongodb": "^6.8.0",
    "mongoose": "^8.5.2",
    "openai": "^4.57.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "@types/bcryptjs": "^2.4.6",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.5.0",
    "@types/pg": "^8.11.6",
    "@types/sequelize": "^4.28.20",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.2.0",
    "@typescript-eslint/parser": "^8.2.0",
    "eslint": "^9.9.0",
    "jest": "^29.7.0",
    "serverless": "^3.38.0",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-offline": "^13.6.0",
    "serverless-webpack": "^5.14.1",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0"
  }
